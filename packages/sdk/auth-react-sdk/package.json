{
  "name": "@accountsystem/auth-react-sdk",
  "version": "1.0.0",
  "description": "Frontend SDK for AccountSystem authentication service",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build:types && npm run build:cjs",
    "build:types": "tsc --emitDeclarationOnly",
    "build:cjs": "esbuild src/index.ts --bundle --platform=node --target=es2020 --format=cjs --outfile=dist/index.js",
    "dev": "npm run build -- --watch",
    "clean": "node -e \"const fs = require('fs'); if (fs.existsSync('dist')) fs.rmSync('dist', { recursive: true });\"",
    "prepublishOnly": "npm run build",
    "test": "echo \"Tests not implemented yet\" && exit 0",
    "link": "npm run build && npm link"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "auth",
    "authentication",
    "frontend",
    "sdk",
    "typescript",
    "oauth",
    "local-auth"
  ],
  "author": "0-kodiya-0",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/react": "^19.1.6",
    "esbuild": "0.25.5",
    "eslint": "^9.28.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  },
  "dependencies": {
    "@types/node": "^22.15.29",
    "axios": "^1.9.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "socket.io-client": "^4.8.1",
    "zustand": "^5.0.5"
  }
}