{
  "name": "todo-app-backend",
  "version": "1.0.0",
  "description": "Todo app backend using auth-node-sdk",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "dev:nodemon": "nodemon --exec tsx src/index.ts",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "test": "echo \"Add tests later\" && exit 0"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.2",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "nodemon": "^3.1.10",
    "rimraf": "^5.0.5",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
